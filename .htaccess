

# Redirect all requests to Laravel's public folder
<IfModule mod_rewrite.c>
    RewriteEngine On

    # If the request is not already in the public folder
    RewriteCond %{REQUEST_URI} !^/public
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

